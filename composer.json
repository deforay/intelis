{
    "name": "deforay/intelis",
    "description": "InteLIS - Open Source LIS",
    "homepage": "https://github.com/deforay/intelis",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "version": "5.3.2",
    "authors": [
        {
            "name": "Deforay",
            "email": "hello@deforay.com",
            "homepage": "https://deforay.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-pdo_sqlite": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-gettext": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "brick/phonenumber": "^0.6.0",
        "crunzphp/crunz": "^3.8",
        "gettext/gettext": "^5.7",
        "gregwar/captcha": "^1.3",
        "guzzlehttp/guzzle": "^7.10",
        "hackzilla/password-generator": "^1.7",
        "halaxa/json-machine": "^1.2",
        "ifsnop/mysqldump-php": "^2.12",
        "laminas/laminas-diactoros": "^3.6",
        "laminas/laminas-httphandlerrunner": "^2.12",
        "laminas/laminas-stratigility": "^4.2",
        "league/csv": "^9.24",
        "maennchen/zipstream-php": "3.1.2",
        "monolog/monolog": "^3.9",
        "nesbot/carbon": "^3.10",
        "nikic/iter": "^2.4",
        "php-di/php-di": "^7.1",
        "phpmailer/phpmailer": "^6.10",
        "phpmyadmin/sql-parser": "^5.11",
        "phpoffice/phpspreadsheet": "^5.0",
        "psr/log": "^3.0",
        "setasign/fpdi": "^2.6",
        "slim/psr7": "^1.7",
        "slim/slim": "^4.15",
        "sqids/sqids": "^0.4.1",
        "symfony/cache": "^7.3",
        "symfony/console": "^7.2",
        "symfony/filesystem": "^7.3",
        "symfony/string": "^6.4",
        "symfony/uid": "^7.3",
        "tecnickcom/tcpdf": "^6.10",
        "thingengineer/mysqli-database-class": "dev-master",
        "voku/anti-xss": "^4.1"
    },
    "suggest": {
        "dcarbone/php-fhir-generated": "Needed for FHIR Parsing"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/classes/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": true,
        "platform": {
            "php": "8.2.29",
            "php-64bit": "8.2.29"
        }
    },
    "require-dev": {
        "kint-php/kint": "^5.1",
        "squizlabs/php_codesniffer": "^3.11",
        "symfony/var-dumper": "^7.1"
    },
    "scripts": {
        "scan": "php bin/scan.php",
        "cleanup": "php bin/cleanup.php 30",
        "version": "php bin/build/generate-version.php",
        "collation": "php bin/setup/change-db-collation.php",
        "db-backup": "php bin/db-tools.php backup",
        "interface": "php bin/interface.php",
        "lock-samples": "php bin/update-sample-status.php",
        "purge-cache": "php app/includes/clear-cache.php",
        "privilege-fix": "php bin/setup/update-privileges.php",
        "audit-tables-fix": "php bin/setup/fix-audit-tables.php",
        "archive-audit-tables": "php app/tasks/archive-audit-tables.php",
        "token": "php bin/token.php",
        "fix-sts": "php bin/setup/fix-sts.php",
        "system-admin": "php bin/setup/system-admin.php",
        "migrate": "php bin/migrate.php -y",
        "sample-generator": "php bin/sample-code-generator.php",
        "force-metadata": "php app/tasks/remote/sts-metadata-receiver.php -ft",
        "metadata-sync": "php app/tasks/remote/sts-metadata-receiver.php && php app/tasks/remote/lab-metadata-sender.php",
        "remote-sync": "php app/tasks/remote/results-sender.php && php app/tasks/remote/requests-receiver.php && php app/tasks/remote/lab-metadata-sender.php",
        "send-results": "php app/tasks/remote/results-sender.php",
        "receive-requests": "php app/tasks/remote/requests-receiver.php",
        "generate-translation-strings": "php app/system/generate-translation-strings.php",
        "post-install": "php bin/migrate.php -y && php bin/setup/update-privileges.php && php bin/setup/fix-app-menu.php && php app/tasks/remote/sts-metadata-receiver.php -f && php app/includes/clear-cache.php && php bin/setup/change-db-collation.php",
        "post-update": "php bin/migrate.php -y && php bin/setup/update-privileges.php && php bin/setup/fix-app-menu.php && php bin/token.php && php app/includes/clear-cache.php && php bin/setup/change-db-collation.php"
    }
}
